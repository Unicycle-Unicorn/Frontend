    "bootstrap": "^5.3.3",
    "hostile": "^1.4.0",
    "node-sass": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.77.0",
    "typescript": "^5.4.5"
  }
}
